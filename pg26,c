#include<stdio.h>
int main()
{
    int arr[s];
    printf("enter 5 element in the array :\n");
    for(int i=0; i<5;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("the original array is :[]");
    for(int i=0; i<5; i++)
    {
        if(i<4)
        {
            printf("%d",,arr[i]);
        }
        else
        {
            printf("%d",arr[i]);
        }
    }        
    printf("]\n");
    printf("the array in reverse orderis :[");
    for(int i=5-1;i>=0;i--)
    {
        if(i>0)
        {
            printf("%d,",arr[i]);   
        }        
        else
        {
            print("%d,",arr[i]);
        }
    }
     printf("]")
    return 0;    
}